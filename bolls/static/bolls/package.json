{
  "name": "bolls",
  "scripts": {
    "start": "imba build --no-hashing -w ./app/index.html",
    "build": "imba build --no-hashing ./app/index.html",
    "babel-build": "npx babel dist/public/__assets__/app/client.js -o dist/public/__assets__/app/main.js",
    "rename": "node -e \"require('fs').rename('dist/public/__assets__/app/main.js', 'dist/public/__assets__/app/client.js', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "b": "npm run build && npm run babel-build && npm run rename"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "imba": "^2.0.0-alpha.129"
  },
  "devDependencies": {
    "@types/node": "^14.14.37"
  },
  "repository": "Bohooslav/bolls",
  "description": "Frontend for Bolls Bible app",
  "version": "1.0.0",
  "homepage": "https://github.com/Bohooslav/bolls",
  "author": "Pavlyshynets Bohuslav (https://github.com/Bohooslav)",
  "license": "ISC"
}